{
  "name": "d2-ui",
  "version": "0.0.8",
  "description": "d2-ui",
  "main": "index.js",
  "scripts": {
    "test": "./node_modules/karma/bin/karma start test/config/karma.config.js --single-run true",
    "test-watch": "./node_modules/karma/bin/karma start test/config/karma.config.js",
    "lint": "./node_modules/eslint/bin/eslint.js src && ./node_modules/eslint/bin/eslint.js --env node,mocha --global expect,sinon --rule 'no-unused-expressions: 0' test && scss-lint",
    "prebuild": "npm test && npm run lint",
    "build": "npm run build-only",
    "build-only": "./node_modules/babel/bin/babel.js src --out-dir lib && compass compile",
    "validate": "npm ls"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/dhis2/d2-ui.git"
  },
  "license": "BSD-3-Clause",
  "devDependencies": {
    "babel": "5.8.21",
    "babel-core": "5.8.22",
    "babel-eslint": "4.0.10",
    "babel-loader": "5.3.2",
    "chai": "3.0.0",
    "coveralls": "2.11.4",
    "d2-testutils": "0.1.7",
    "es6-module-loader": "0.17.6",
    "eslint": "1.2.0",
    "eslint-config-dhis2": "0.0.5",
    "eslint-plugin-react": "3.2.3",
    "fbjs": "0.2.1",
    "istanbul": "0.3.18",
    "karma": "0.13.10",
    "karma-babel-preprocessor": "5.2.1",
    "karma-chai": "0.1.0",
    "karma-coverage": "0.5.0",
    "karma-mocha": "0.2.0",
    "karma-mocha-reporter": "1.1.1",
    "karma-phantomjs-launcher": "0.2.1",
    "karma-sinon": "1.0.4",
    "karma-sinon-chai": "1.0.0",
    "karma-systemjs": "0.7.2",
    "lodash.isstring": "3.0.1",
    "loglevel": "1.4.0",
    "material-ui": "0.12.2",
    "mocha": "2.2.5",
    "phantomjs": "1.9.18",
    "phantomjs-polyfill": "0.0.1",
    "precommit-hook": "3.0.0",
    "react": "0.13.3",
    "react-dom": "0.14.0",
    "react-stub-context": "0.3.0",
    "react-tap-event-plugin": "0.1.7",
    "sinon": "1.15.4",
    "sinon-chai": "2.8.0",
    "systemjs": "0.18.11",
    "webpack": "1.12.2",
    "webpack-dev-server": "1.11.0"
  },
  "dependencies": {
    "classnames": "2.1.3",
    "d2": "0.0.20",
    "d2-flux": "0.4.0",
    "d2-utils": "0.0.4",
    "lodash.isfinite": "3.2.0",
    "lodash.isnumber": "3.0.1",
    "rx": "3.1.2"
  },
  "pre-commit": [
    "lint",
    "validate",
    "test"
  ]
}
